{
  "name": "tree-sitter-glint",
  "version": "0.2.12",
  "description": "Parser for the Glint Programming Language (LCC)",
  "repository": "https://github.com/lensplaysgames/tree-sitter-glint",
  "funding": "https://www.paypal.com/donate/?hosted_button_id=62KQ4GX6HFTNG",
  "license": "MIT",
  "author": {
    "name": "lens_r",
    "email": "lensplaysgames@gmail.com",
    "url": "https://github.com/LensPlaysGames"
  },
  "type": "module",
  "main": "bindings/node",
  "types": "bindings/node",
  "keywords": [
    "incremental",
    "parsing",
    "tree-sitter",
    "glint"
  ],
  "files": [
    "grammar.js",
    "tree-sitter.json",
    "binding.gyp",
    "prebuilds/**",
    "bindings/node/*",
    "queries/*",
    "src/**",
    "*.wasm"
  ],
  "dependencies": {
    "node-addon-api": "^8.5.0",
    "node-gyp-build": "^4.8.4"
  },
  "devDependencies": {
    "prebuildify": "^6.0.1",
    "tree-sitter": "^0.22.4",
    "tree-sitter-cli": "^0.25.10"
  },
  "peerDependencies": {
    "tree-sitter": "^0.22.4"
  },
  "peerDependenciesMeta": {
    "tree-sitter": {
      "optional": true
    }
  },
  "scripts": {
    "install": "node-gyp-build",
    "prestart": "tree-sitter build --wasm",
    "start": "tree-sitter playground",
    "test": "node --test bindings/node/*_test.js"
  }
}
